					<span>Просто начните печатать сообщение, а после отправьте</span>
				</div>
				<!-- sections_headline END-->
				<div class="section_6__wrapper-social">
					<% section6.forEach((el, index) => { %>
					<div
						class="section_6__social-messengers <%if (index == 1) {%>wp<%} else if (index == 0) {%>vk<%} else {%>tg<%}%>"
					>
						<div class="section_6__messenger">
							<a href="<%- el.link%>"><img src="<%- el.img%>" alt="" /></a>
						</div>
						<!-- section_6__messenger -->

						<div class="section_6__soc-mes_content">
							<div
								class="section_6__content-header <%if(index == 0){%>icon_vk<%}%>"
								style="background-color: #<%=el.color.split(' ') [0]%>"
							>
								<img
									src="img/section_6_messengers/<%= el.name %>/back.png"
									alt=""
									class="section_6__icon_img"
								/>

								<div
									class="section_6__content__ava-name-been"
									id="section_6__content-<%= el.name%>"
								>
									<img src="/img/section_6_messengers/ava.png" alt="" />
									<div class="section_6__content-name-been">
										<span class="section_6__content__name">Екатерина</span>
										<div
											class="section_6__content__been"
											style="
										<% if (el.name == 'vk') { %>
											color: #8F8F8F;
											<%} else {%>
												color: #A8CAE3;
												<%}%>
										"
										>
											<%if (el.name != "wp") {%> <%if (el.name == "tg") {%>
											<span>был(а) недавно</span>
											<%} else {%>
											<span>была в сети в 18.30</span>
											<%}%> <%}%>
										</div>
									</div>
								</div>

								<div class="section_6__wrapper-icon">
									<%if (el.name == "wp") {%>
									<img src="img/section_6_messengers/wp/video.png" alt="" />
									<%}%>

									<img
										src="img/section_6_messengers/<%= el.name %>/call.png"
										alt=""
										class="section_6__call"
									/>

									<img
										src="img/section_6_messengers/<%= el.name %>/settings.png"
										alt=""
										class="section_6__img-setting"
									/>
								</div>
							</div>
							<!-- section_6__content-header ENd -->

							<div class="section_6__content-body">
								<% for (let i = 0; i < 10; i++) { %> <% if
								(el.kate.split('|').length > i && el.student.split("|").length >
								i) {%>
								<div class="section_6__dialog-wrapper">
									<div class="section_6__kate section_6__kate_<%= el.name %>">
										<%=el.kate.split("|")[i]%>
										<span class="section_6__time-ok__kate">
											<div>18.22</div>
										</span>
									</div>

									<div
										class="section_6__student section_6__student_<%= el.name %>"
									>
										<div>
											<%=el.student.split("|")[i]%>

											<span class="section_6__time-ok__student">
												<div>
													18.24
													<img src="/img/section_6_messengers/ok.png" alt="" />
												</div>
											</span>
										</div>
									</div>
								</div>
								<%}%> <%}%>
							</div>

							<div class="section_6__content-footer">
								<div class="section_6__footer-wrapper">
									<div class="section_6_footer-icons-textArea">
										<div class="section_6__icons-left">
											<img
												src="/img/section_6_messengers/<%= el.name%>/smile.png"
												alt=""
											/>

											<% if (el.name == "wp") { %>
											<img
												src="/img/section_6_messengers/<%= el.name%>/plus.png"
												alt=""
											/>

											<%}%>
										</div>
										<!-- section_6__icons-left END -->

										<input type="text" placeholder="Написать сообщение" />

										<% if (el.name == "tg") { %>
										<div class="section_6__icons-right">
											<img
												src="/img/section_6_messengers/<%= el.name%>/attach.png"
												alt=""
											/>
										</div>
										<%}%>

										<!-- section_6__icons-right END -->
									</div>
									<!-- section_6_footer-icons-textArea END -->

									<div class="section_6__send-message">
										<a href="">
											<img
												src="img/section_6_messengers/<%= el.name%>/send.png"
												alt=""
											/>
										</a>
									</div>
								</div>
							</div>
						</div>
						<!-- section_6__soc-mes_content END -->
					</div>
					<!-- section_6__social-messengers END -->
					<%})%>
				</div>
				<!-- section_6__wrapper-social END  -->
			</div>
		</section>
		<!-- section_6 END -->

		<section class="section_8" id="reviews">
