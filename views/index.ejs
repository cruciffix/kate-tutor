						<h3><%= el.headline%></h3>

						<!-- END close__open -->
					</div>

					<div
						class="section_2__item_wrapper section_2__content_description number_item"
					>
						<div class="close__open">
							<div class="section_2__open">
								<p class="section_2__description"><%- el.description%></p>
								<span href="" class="roll_up<%- el.id %>">Свернуть</span>
							</div>

							<div class="section_2__close">
								<span class="see__more section_2__<%-el.id%>"
									>Посмотреть дальше</span
								>
							</div>
						</div>

						<!-- END close__open -->
					</div>
				</div>

				<% }) %>
			</div>
		</section>
