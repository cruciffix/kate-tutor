							>
								<%for (let i = 1; i <= 4; i ++) {%>
								<div
									class="section_8__div <%if (item == 0) {%> section_8__div-<%= i%> <%}%>"
								></div>

								<%}%>
							</div>

							<div
								class="section_8__rating-slider <%if (item != 0) { %>section_8__last<%}%>"
							>
								<!-- UL -->
								<div class="section_8__rainting-content">
									<% if (item == 0) {%> <% section8.forEach(el => { %> <%if
									(el.name == "avito") { %>
									<div
										class="section_8__wrapper-content section_8__wrapper-content-avito"
									>
										<div class="section_8__content-headline">
											<img class="section_8__ava" src="/img/section_8/ava.png" alt="" />

											<div class="section_8__headline-rating">
												<span>Екатерина Убейкина</span>

												<div>
													<% for (let i = 0; i < el.rating; i++) {%>
													<img src="img/section_8/star.png" alt="" />
													<%}%>
												</div>
											</div>
											<!-- section_8__headline-rating" ENd -->
										</div>
										<!-- section_8__content-headline END -->

										<div class="section_8__body">
											<p><%= el.body%></p>
										</div>
									</div>
									<%} %> <%}) %> <%} else {%> <% section8.forEach(el => { %>
									<%if (el.name == "profi") { %>
									<div
										class="section_8__wrapper-content section_8__wrapper-content-profi"
									>
										<div class="section_8__content-headline">
											<img src="/img/section_8/ava.png" alt="" />

											<div class="section_8__headline-rating">
												<span>Екатерина Убейкина</span>

												<div>
													<% for (let i = 0; i < el.rating; i++) {%>
													<img src="img/section_8/star.png" alt="" />
													<%}%>
												</div>
											</div>
											<!-- section_8__headline-rating" ENd -->
										</div>
										<!-- section_8__content-headline END -->

										<div class="section_8__body">
											<p><%= el.body%></p>
										</div>
									</div>
									<%} %> <%}) %> <%}%>
								</div>

								<div class="section_8__wrappper-slider">
									<div class="section_8__horizontal-line">
										<!-- slider -->
										<div class="section_8__circle" id=""></div>
										<!-- thumb -->
									</div>
								</div>
							</div>
						</div>
						<%})%>
						<!-- section_8__slider end -->
					</div>
				</div>
			</div>
		</section>
		<!-- section_8 END -->

